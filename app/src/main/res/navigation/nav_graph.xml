<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listFragment3">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.utsmap.LoginFragment"
        android:label="Login Fragment"
        tools:layout="@layout/fragment_login" />

    <fragment
        android:id="@+id/signupFragment"
        android:name="com.example.utsmap.SignupFragment"
        android:label="Sign Up Fragment"
        tools:layout="@layout/fragment_signup" />
    <fragment
        android:id="@+id/listFragment3"
        android:name="com.example.utsmap.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/action_listFragment3_to_detailFragment2"
            app:destination="@id/detailFragment2" />
    </fragment>

    <fragment
        android:id="@+id/detailFragment2"
        android:name="com.example.utsmap.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />

    <fragment
        android:id="@+id/favouritesFragment2"
        android:name="com.example.utsmap.ProfileFragment"
        android:label="fragment_favourite"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/danceFragment2"
        android:name="com.example.utsmap.DanceFragment"
        android:label="fragment_dance" />
</navigation>
